{
  "name": "feng-rz",
  "version": "1.0.0",
  "description": "凤凰金融-融资",
  "scripts": {
    "start": "NODE_ENV=production NODE_PATH=./src node ./pm2.js",
    "list": "pm2 list",
    "stop": "pm2 kill",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "watch-client": "UV_THREADPOOL_SIZE=100 NODE_PATH=./src PORT=3000 node webpack/webpack-dev-server.js",
    "start-dev": "NODE_PATH=./src NODE_ENV=development node ./bin/server.js"
  },
  "dependencies": {
    "base64-url": "^1.2.2",
    "body-parser": "^1.14.1",
    "compression": "^1.6.0",
    "console-polyfill": "^0.2.2",
    "cookie-parser": "^1.4.3",
    "es3ify-loader": "^0.2.0",
    "es5-shim": "^4.5.9",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "file-loader": "^0.8.5",
    "hoist-non-react-statics": "^1.0.3",
    "http-proxy": "^1.12.0",
    "invariant": "^2.2.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lru-memoize": "^1.0.0",
    "map-props": "^1.0.0",
    "multireducer": "^2.0.0",
    "piping": "^0.3.0",
    "pm2": "^1.1.3",
    "pretty-error": "^1.2.0",
    "react": "^0.14.2",
    "react-addons": "^0.9.0",
    "react-dom": "^0.14.1",
    "react-helmet": "^2.2.0",
    "react-inline-css": "^2.0.0",
    "react-redux": "^4.0.0",
    "react-router": "2.0.0",
    "redux": "^3.0.4",
    "redux-form": "^3.0.12",
    "scroll-behavior": "^0.3.2",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "signals": "^1.0.0",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "superagent": "^1.4.0",
    "url-loader": "^0.5.7",
    "warning": "^2.1.0",
    "webpack-isomorphic-tools": "^2.2.18"
  },
  "devDependencies": {
    "concurrently": "^2.1.0",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-antd": "^0.4.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-ie8-classes": "^0.1.8",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-plugin-typecheck": "^3.6.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "clean-webpack-plugin": "^0.1.6",
    "css-loader": "^0.23.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "0.0.4",
    "json-loader": "^0.5.4",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "react-a11y": "^0.2.6",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "timekeeper": "0.0.5",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015-loose",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "add-module-exports",
      "transform-decorators-legacy",
      "transform-react-display-name"
    ],
    "env": {
      "development": {
        "plugins": [
          "typecheck",
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "react-transform-catch-errors",
                  "imports": [
                    "react",
                    "redbox-react"
                  ]
                }
              ]
            }
          ]
        ]
      }
    }
  }
}
